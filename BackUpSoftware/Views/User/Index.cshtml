@using System.Drawing.Imaging
@using System.IO;
@model BackUpSoftware.ViewModel.FolderViewModel
@{
    ViewBag.Title = "Index";
}

<div class="container">
  
    <div class="view-account">
        <section class="module">
            <div class="module-inner">
                <div class="side-bar">
                    <div class="user-info">
                        <img class="img-profile img-circle img-responsive center-block" src="https://bootdey.com/img/Content/avatar/avatar6.png" alt="">
                        <ul class="meta list list-unstyled">
                            <li class="name">
                            @Model.User.FirstName
                           
                            <li class="email"><a href="#">@Model.User.Email</a></li>
            
                        
                        </ul>
                    </div>
                    <nav class="side-menu">
                        <ul class="nav">
                            <!--<li><a href="#"><span class="fa fa-user"></span> Profile</a></li>
